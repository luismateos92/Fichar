{
    "name": "luismateos92/fichar",
    "description": "Script fichar automáticamente todos los días.",
    "keywords": ["fichar", "automático", "automáticamente", "luismateos92"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "league/flysystem": "^1.0",
        "monolog/monolog": "^1.23",
        "nesbot/carbon": "^2.16",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "1.0.*",
        "phpro/grumphp": "^0.14.0",
        "phpunit/phpunit": "~7.0"
    },
	"autoload": {
		"psr-4": {
			"LuisMateos92\\Fichar\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
	},
    "minimum-stability": "stable"
}
